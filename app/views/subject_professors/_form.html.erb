<%= form_for :subject_professor, url: {action: "create"}, html: {method: :post} do |f| %>
    <div class = "form-group">
	<%= f.label :subject_id, class: "control-label" %>
	<%= f.number_field :subject_id, class: "form-control" %>
    </div>

    <div class = "form-group">
	<%= f.label :professor_id, class: "control-label" %>
	<%= f.number_field :professor_id, class: "form-control" %>
    </div>

    <div class = "form-group">
	<%= f.submit "Submit", class: "btn btn-primary" %>
    </div>

<% end %>
